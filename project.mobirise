        {
          "_name": "footer2",
          "_customHTML": "<section class=\"footer2 cleanm4_footer2\" group=\"Footers\" always-bottom global once=\"footer\">\n\n\t<mbr-parameters>\n\t\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t\t<header>Size</header>\n\t\t<input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n\t\t<input inline type=\"range\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"2\">\n\t\t<input inline type=\"range\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"2\">\n\t\t<select title=\"Icons\" name=\"iconsCount\" condition=\"showIcons\">\n\t\t\t<option value=\"1\">1</option>\n\t\t\t<option value=\"2\">2</option>\n\t\t\t<option value=\"3\">3</option>\n\t\t\t<option value=\"4\" selected>4</option>\n\t\t\t<option value=\"5\">5</option>\n\t\t</select>\n\t\t<header>Show / Hide</header>\n\t\t<input type=\"checkbox\" title=\"Logo\" name=\"showLogo\">\n\t\t<input type=\"checkbox\" title=\"Copyright\" name=\"showCopyright\" checked>\n\t\t<input type=\"checkbox\" title=\"Icons\" name=\"showIcons\" checked>\n\t\t<input type=\"checkbox\" title=\"Border Top\" name=\"showBorderTop\" checked>\n\t\t<header>Color</header>\n\t\t<input type=\"color\" title=\"Border Top\" name=\"borderTopColor\" value=\"#d9dade\" condition=\"showBorderTop\">\n\t\t<input type=\"color\" title=\"Circle\" name=\"circleColor\" value=\"#c1c2c5\" condition=\"showIcons\">\n\t\t<input type=\"color\" title=\"Circle Bg Hover\" name=\"circleHoverColor\" value=\"#232323\" condition=\"showIcons\">\n\t\t<input type=\"color\" title=\"Icon Hover\" name=\"iconHoverColor\" value=\"#ffffff\" condition=\"showIcons\">\n\t\t<header>Background</header>\n\t\t<fieldset type=\"background\" name=\"bg\" parallax>\n\t\t\t<input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/cleanm4/components/_images/background1.jpg\" parallax>\n\t\t\t<input type=\"color\" title=\"Color\" value=\"#efefef\" selected>\n\t\t</fieldset>\n\t\t<input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n\t\t<input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#efefef\" condition=\"overlay&&bg.type !== 'color'\">\n\t\t<input type=\"range\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay&&bg.type !== 'color'\">\n\t\t<!-- End block parameters -->\n\t</mbr-parameters>\n\n\t<div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n\t</div>\n\n\t<div mbr-class=\"{'container-fluid': fullWidth, 'container': !fullWidth }\">\n\t\t<div class=\"media-container-row\">\n\t\t\t<div class=\"col-lg-4 logo-footer mb-3 mt-3 align-left\">\n\t\t\t\t<img class=\"img-logo-footer\" src=\"@ADDONS_PATH@/cleanm4/components/_images/logo.png\" alt=\"Mobirise\" mbr-if=\"showLogo\">\n\t\t\t\t<p class=\"mbr-fonts-style mbr-bold text-logo-footer mb-0\" mbr-theme-style=\"display-5\" data-app-selector=\".text-logo-footer\">Bernard Jean-Luc Audemard</p>\n\t\t\t</div>\n\t\t\t<div class=\"col-lg-4 copyright mb-3 mt-3\">\n\t\t\t\t<p class=\"mbr-fonts-style mbr-regular text-copyright\" mbr-theme-style=\"display-4\" mbr-if=\"showCopyright\" data-app-selector=\".text-copyright\">Thank you for viewing my portfolio!<br>Copyright Â© 2021<br></p>\n\t\t\t</div>\n\t\t\t<div class=\"social-media col-lg-4 col-sm-12\" mbr-if=\"showIcons\">\n\t\t\t\t<ul>\n\t\t\t\t\t<li mbr-if=\"iconsCount > 0\">\n\t\t\t\t\t\t<a class=\"icon-transition\" href=\"#\">\n\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont socicon-facebook socicon\">\n\t\t\t\t\t\t\t</span>\n\t\t\t\t\t\t</a>\n\t\t\t\t\t</li>\n\t\t\t\t\t<li mbr-if=\"iconsCount > 1\">\n\t\t\t\t\t\t<a class=\"icon-transition\" href=\"#\">\n\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont socicon-instagram socicon\"></span>\n\t\t\t\t\t\t</a>\n\t\t\t\t\t</li>\n\t\t\t\t\t<li mbr-if=\"iconsCount > 2\">\n\t\t\t\t\t\t<a class=\"icon-transition\" href=\"#\">\n\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont socicon-linkedin socicon\"></span>\n\t\t\t\t\t\t</a>\n\t\t\t\t\t</li>\n\t\t\t\t\t<li mbr-if=\"iconsCount > 3\">\n\t\t\t\t\t\t<a class=\"icon-transition\" href=\"#\">\n\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont mdi-notification-phone-forwarded\"></span>\n\t\t\t\t\t\t</a>\n\t\t\t\t\t</li>\n\t\t\t\t\t<li mbr-if=\"iconsCount > 4\">\n\t\t\t\t\t\t<a class=\"icon-transition\" href=\"#\">\n\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont socicon-rss socicon\" style></span>\n\t\t\t\t\t\t</a>\n\t\t\t\t\t</li>\n\t\t\t\t</ul>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n</section>",
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@showBorderTop)": {
              "border-top": "1px solid @borderTopColor"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            "p": {
              "text-align": "center"
            },
            ".text-copyright": {
              "width": "100%",
              "margin-bottom": "0",
              "color": "#232323"
            },
            ".img-logo-footer": {
              "margin-right": "1rem",
              "max-width": "3rem"
            },
            ".text-logo-footer": {
              "display": "inline-block",
              "vertical-align": "middle",
              "color": "@primaryColor"
            },
            ".logo-footer": {
              "line-height": "normal"
            },
            ".copyright": {
              "display": "flex",
              "align-items": "center"
            },
            ".social-btns": {
              "display": "flex",
              "align-items": "center",
              "justify-content": "center"
            },
            ".media-container-row": {
              "align-items": "center",
              "justify-content": "space-between"
            },
            ".icon-transition span": {
              "color": "@infoColor",
              "display": "block",
              "text-align": "center",
              "height": "3em",
              "line-height": "3em",
              "border-radius": "50%",
              "border": "1px solid @circleColor",
              "width": "3em",
              "transition": "all 0.3s ease-out 0s",
              "font-size": "14px"
            },
            ".icon-transition span:hover": {
              "background-color": "@circleHoverColor",
              "color": "@iconHoverColor",
              "-webkit-animation-name": "animation-pulse",
              "animation-name": "animation-pulse",
              "-webkit-animation-duration": "1s",
              "animation-duration": "1s",
              "-webkit-animation-timing-function": "linear",
              "animation-timing-function": "linear",
              "-webkit-animation-iteration-count": "infinite",
              "animation-iteration-count": "infinite",
              "opacity": ".9"
            },
            ".social-media": {
              "display": "flex",
              "justify-content": "flex-end",
              "ul": {
                "margin": "0",
                "padding": "0",
                "li": {
                  "padding": "4px",
                  "display": "inline-block"
                }
              }
            },
            "@-webkit-keyframes animation-pulse": {
              "25%": {
                "transform": "scale(1.1)"
              },
              "75%": {
                "transform": "scale(0.9)"
              }
            },
            "@keyframes animation-pulse": {
              "25%": {
                "transform": "scale(1.1)"
              },
              "75%": {
                "transform": "scale(0.9)"
              }
            },
            "@media (max-width: 991px)": {
              ".logo-footer": {
                "text-align": "center"
              },
              ".social-media": {
                "justify-content": "center"
              }
            }
          },
